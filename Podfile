# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'Chips' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  ## UI
  pod 'SnapKit'
  pod 'Localize-Swift'
  pod 'Hero'
  pod 'KVNProgress'
  pod 'Caishen'
  pod 'CardIO'
  pod 'Shimmer'

  ## Utils
  pod 'Crashlytics'
  pod 'CryptoSwift'
  pod 'Kingfisher'
  pod 'SwiftyRSA'
  pod 'ReachabilitySwift'
  pod 'SwiftLint'

  target 'ChipsTests' do
    inherit! :search_paths
    # Pods for testing
  end

end

target 'Potato' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Potato

  target 'PotatoTests' do
    inherit! :search_paths
    # Pods for testing
  end

